<Window
    x:Class="SoundEditorControl.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:soundEditorControl="clr-namespace:SoundEditorControl"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <DockPanel LastChildFill="True">
        <Button Click="ButtonBase_OnClick" DockPanel.Dock="Bottom">Read values</Button>
        <soundEditorControl:SoundEditor x:Name="mySoundEditor" />
    </DockPanel>
</Window>
